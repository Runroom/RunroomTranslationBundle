{
    "name": "runroom-packages/translation-bundle",
    "type": "symfony-bundle",
    "description": "Manage translations on the Sonata Admin interface",
    "keywords": [
        "translation",
        "symfony",
        "doctrine",
        "admin",
        "sonata"
    ],
    "homepage": "https://github.com/runroom/RunroomTranslationBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "software@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/orm": "^2.9",
        "doctrine/persistence": "^2.0",
        "knplabs/doctrine-behaviors": "^2.0.6",
        "symfony/asset": "^4.4 || ^5.3",
        "symfony/config": "^4.4 || ^5.3",
        "symfony/dependency-injection": "^4.4 || ^5.3",
        "symfony/form": "^4.4 || ^5.3",
        "symfony/http-kernel": "^4.4.13 || ^5.3",
        "symfony/translation": "^4.4 || ^5.3",
        "symfony/validator": "^4.4 || ^5.3",
        "symfony/yaml": "^4.4 || ^5.3",
        "twig/twig": "^2.12 || ^3.0"
    },
    "conflict": {
        "a2lix/translation-form-bundle": "<3.0.4",
        "friendsofsymfony/ckeditor-bundle": "<2.2",
        "sonata-project/admin-bundle": "<3.103",
        "zenstruck/foundry": ">=1.13"
    },
    "require-dev": {
        "a2lix/auto-form-bundle": "^0.3.1",
        "a2lix/translation-form-bundle": "^3.0.4",
        "friendsofsymfony/ckeditor-bundle": "^2.2",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpunit/phpunit": "^9.5",
        "runroom-packages/testing": "^0.13@dev",
        "sonata-project/admin-bundle": "^3.103 || ^4.0@rc",
        "sonata-project/doctrine-orm-admin-bundle": "^3.34 || ^4.0@rc",
        "symfony/framework-bundle": "^4.4 || ^5.3",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/security-bundle": "^4.4 || ^5.3",
        "symfony/twig-bundle": "^4.4 || ^5.3",
        "zenstruck/foundry": "^1.10"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.13-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Runroom\\TranslationBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\TranslationBundle\\Tests\\": "tests/"
        }
    }
}
